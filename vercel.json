{
    "buildCommand": "npm run build",
    "outputDirectory": ".next",
    "framework": "nextjs",
    "regions": ["iad1"],
    "functions": {
        "src/app/api/**/*.ts": {
            "runtime": "nodejs18.x"
        }
    },
    "crons": [
        {
            "path": "/api/cron/process-scheduled",
            "schedule": "0 13 * * *"
        }
    ]
}